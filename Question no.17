#include<stdio.h>

int main()
{
   int i,j;
   int n=8;
   for(i=1;i<=n;i++){
      for(j=1;j<=(n*2)-1;j++){ 
         if(j==i|| j==(n*2)-i || i==1){
            printf(" * ");
         }
         else{
            printf("   ");
         }
      }

      printf("\n");//move to next line
      }

   return 0;
}
