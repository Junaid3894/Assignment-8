#include<stdio.h>

int main()
{
   int i,j;
   int k;
   int n=8;
   for(i=1;i<=n;i++){
      k=1; 
      for(j=1;j<=n*2-1;j++){
         if(j>=i && j<=(n*2)-i){
            printf(" %d ",k);
            j>=n ? k--:k++;
         }
         else{
            printf("   ");
         }
         
      }
      printf("\n");//move to next line
      }

   return 0;
}
