#include<stdio.h>

int main()
{
   int i,j;
   int k;
   int n=7;
   for(i=1;i<=n;i++){
      k=64; 
      for(j=1;j<=(n*2)-1;j++){

      j>=n+1 ?k--:k++;
      if(j<=(n+1)-i || j>=(n-1)+i){
            printf(" %c ",k);

         }
         else{
            printf("   ");
         }
         
      }
      printf("\n");//move to next line
      }

   return 0;
}
