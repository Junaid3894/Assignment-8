#include<stdio.h>

int main()
{
   int i,j;
   int n=5;
   for(i=1;i<=n;i++){
      for(j=1;j<=n;j++){ 
         if(j==1 || j==i || i==n){
            printf(" * ");
         }
         else{
            printf("   ");
         }
      }

      printf("\n");//move to next line
      }

   return 0;
}
